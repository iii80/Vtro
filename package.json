{
  "name": "vtro",
  "version": "0.0.1",
  "description": "A simple trojan GUI",
  "main": "main.js",
  "build": {
    "productName": "Vtro",
    "asar": false,
    "compression": "store",
    "win": {
      "target": [
        "zip"
      ]
    },
    "extraFiles": [
      "tray.ico",
      {
        "from": "extra-trojan/",
        "to": "trojan/"
      }
    ],
    "files": [
      "lib/",
      "index.html",
      "main.js",
      "tray.ico"
    ]
  },
  "scripts": {
    "start": "electron . ",
    "webpack": "webpack --config ./src/config/webpack.config.js",
    "watch": "webpack --config ./src/config/webpack.config.js --watch",
    "packager": "electron-packager .  Vtro  --out ./ --overwrite --ignore=node_modules",
    "dist": "electron-builder --win --x64"
  },
  "repository": "https://github.com/saywhere/Vtro",
  "keywords": [
    "Electron",
    "trojan",
    "GUI",
    "demo"
  ],
  "author": "wk989898",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^3.5.1",
    "electron": "^8.0.0",
    "electron-builder": "^22.4.1",
    "electron-packager": "^14.2.1",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "element-ui": "^2.13.0",
    "file-loader": "^6.0.0",
    "style-loader": "^1.1.3",
    "url-loader": "^4.0.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "webpack": "^4.42.1"
  }
}
